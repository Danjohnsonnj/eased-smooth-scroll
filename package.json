    "babel-jest": "^26.6.3",
    "electron": "^11.1.0",
    "jest": "^26.6.3",
    "less": "^3.12.2",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "js-easing-functions": "^1.0.3"
